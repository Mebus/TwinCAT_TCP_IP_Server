	nRemotePort:= Port, 
	cbLen:= LEN(sSend), 
	pSrc:= ADR(sSend), 
	bExecute:= , 
	tTimeout:= , 
	bBusy=> , 
	bError=> , 
	nErrId=> );
	
	
fb1(
