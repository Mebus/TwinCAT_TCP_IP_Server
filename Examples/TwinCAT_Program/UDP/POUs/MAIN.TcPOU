	NetId: STRING := '';
	HostAdr: STRING := '127.0.0.1';
	Port: UDINT := 5005;
	hSocket: T_HSOCKET;
	
	sReceive : STRING;
	sSend : STRING := 'Neomuhae';
	
	
	
	
	fb1:FB_SocketConnect;
	fb2:FB_SocketReceive;
	fb3:FB_SocketSend;
END_VAR
]]></Declaration>
